.navbar-inverse .brand 
    float right
.navbar-inner
    margin 0px
    -webkit-border-radius 0px
    -moz-border-radius 0px
    border-radius 0px
h1.master-title
    font-size 128px
    font-family "Courier New", Courier, monospace
    margin-bottom 0.2em
h2.master-subtitle
    padding-left 0.2em
.master-stripe
    margin-top 64px
    margin-bottom 64px
.task
    padding-top 1em
    background #DDF
    border-color #AAA
    border-style solid
    border-width 1px
    position relative
    -webkit-border-radius 1em
    -moz-border-radius 1em
    border-radius 1em
h4.task-top     
    background #EEE
    border-color #AAA
    border-style solid
    border-width 1px
    font-size 38px
    padding 0.5em
    margin-left 2em
    margin-right 2em
    background #FFF
    -webkit-border-radius 1em
    -moz-border-radius 1em
    border-radius 1em
h4.task-bottom
    font-size 24px
    padding 0.5em
