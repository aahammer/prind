html,body{
   position relative
   margin:0;
   padding:0;
   height: 100%
   min-height:100%
}
#footer {
   
   bottom:0;
   width:100%;
   height:2em;  
   background:#222;
}

.comming-soon

    -webkit-border-radius 12px
    -moz-border-radius 12px
    border-radius 12px
    position absolute
    background #000
    opacity 0.8
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    font-size 32px
    line-height 1em
    color #DDD
    font-weight bold

.footer-right
    float right
    padding-right 1em
    padding-top 1em
    
.what-text
    font-size 12px
    padding-left 20px
    padding-right 1em
    font-weight normal
#whats-this:hover
    background #940
    
#whats-this
    position:fixed
    bottom:0
    left:0
    background #630
    width:350px
    height:50px
    color white
    font-size 20px
    font-weight bold
    font-family "Courier New", Courier, monospace
    -webkit-border-top-right-radius: 1em;
    -moz-border-radius-topright: 1em;
    border-top-right-radius: 1em;
    
@media (max-width: 480px)

  .navbar-inverse .brand
        display:none
  h1.master-title 
        font-size 96px
  .master-stripe
        margin-top 32px
        margin-bottom 32px

